{
  "name": "tstask",
  "description": "task management cli with typescript and oclif framework",
  "version": "0.0.0",
  "private": true,
  "author": "kis9a ",
  "bin": {
    "tstask": "./bin/run"
  },
  "bugs": "https://github.com/kis9a/tstask/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-update": "^1.3.10",
    "@types/inquirer": "^7.3.1",
    "@types/lowdb": "^1.0.9",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "inquirer": "^7.3.3",
    "lowdb": "^1.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "tstask",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update"
    ]
  },
  "repository": "kis9a/tstask",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
